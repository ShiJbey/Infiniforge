{
  "name": "infiniforge",
  "private": false,
  "version": "2.0.2",
  "description": "Typescript/Javascript library for procedurally generating 3D fantasy swords",
  "main": "./build/cjs/index.js",
  "module": "./build/esm/index.js",
  "readme": "README.md",
  "scripts": {
    "start": "node server/server.js",
    "build": "tsc -p tsconfig.esm.json && tsc -p tsconfig.cjs.json",
    "build:server": "npx webpack --config webpack.config.js",
    "build:all": "npm run build && npm run build:server",
    "build:check": "tsc --noEmit",
    "build:watch": "tsc --noEmit --watch",
    "prepublishOnly": "npm run build:all",
    "typedoc": "typedoc --out docs ./src/"
  },
  "bin": {
    "infiniforge-server": "server/server.js"
  },
  "directories": {
    "build": "./build",
    "server": "./server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShiJbey/Infiniforge.git"
  },
  "keywords": [
    "forge",
    "procedural",
    "generation",
    "sword",
    "threejs",
    "infiniforge"
  ],
  "author": "Shi Johnson-Bey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ShiJbey/Infiniforge/issues"
  },
  "homepage": "https://github.com/ShiJbey/Infiniforge#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "browserslist": "^4.6.6",
    "colors": "^1.3.3",
    "commander": "^6.1.0",
    "express": "^4.17.1",
    "heap": "^0.2.6",
    "heap-js": "^2.1.4",
    "lodash": "^4.17.20",
    "seedrandom": "^3.0.5",
    "serve-favicon": "^2.5.0",
    "serve-static": "^1.14.1",
    "three": "^0.124.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.8",
    "@types/lodash": "^4.14.161",
    "@types/node": "^14.14.20",
    "@types/seedrandom": "^2.4.28",
    "@types/serve-favicon": "^2.5.0",
    "@types/three": "^0.103.2",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.14.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-airbnb-typescript-prettier": "^4.1.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.14",
    "typescript": "^4.1.3",
    "webpack": "^5.12.3",
    "webpack-cli": "^4.3.1"
  }
}
