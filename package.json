{
  "name": "infiniforge",
  "private": false,
  "version": "2.2.0",
  "description": "Procedurally generate 3D fantasy sword meshes",
  "main": "./dist/cjs/infiniforge.js",
  "module": "./dist/esm/infiniforge.js",
  "readme": "README.md",
  "scripts": {
    "build": "rollup --config --bundleConfigAsCjs ./rollup.config.js",
    "lint": "eslint ./src/infiniforge",
    "format": "prettier --write ./src/infiniforge",
    "prepublishOnly": "npm run build",
    "build:docs": "typedoc --out docs ./src/",
    "clean": "rimraf ./dist ./docs"
  },
  "directories": {
    "dist": "./dist"
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE.txt",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShiJbey/Infiniforge.git"
  },
  "keywords": [
    "procedural generation",
    "pcg",
    "fantasy",
    "sword",
    "DnD",
    "TTRPG"
  ],
  "author": "Shi Johnson-Bey <shijbey@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ShiJbey/Infiniforge/issues"
  },
  "homepage": "https://github.com/ShiJbey/Infiniforge#readme",
  "dependencies": {
    "body-parser": "^1.20.1",
    "colors": "^1.4.0",
    "commander": "^9.4.1",
    "express": "^4.18.2",
    "seedrandom": "^3.0.5"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.3.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.15",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "@types/seedrandom": "^3.0.4",
    "@types/serve-favicon": "^2.5.3",
    "@types/three": "^0.148.0",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "lodash": "^4.17.21",
    "prettier": "2.8.2",
    "rimraf": "^3.0.2",
    "rollup": "^3.9.1",
    "three": "^0.148.0",
    "typedoc": "^0.23.23",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "lodash": "^4.17.21",
    "three": "^0.148.0"
  }
}
