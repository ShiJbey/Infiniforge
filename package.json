{
  "name": "infiniforge",
  "private": true,
  "version": "2.0.0",
  "description": "InfiniForge is a NodeJS REST API that returns JSON representations of procedurally generated 3D fanstasy swords",
  "main": "./build/Infiniforge.js",
  "scripts": {
    "start": "node server/server.js",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "tsc",
    "typedoc": "typedoc --out docs ./src/"
  },
  "bin": {
    "infiniforge-server": "server/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShiJbey/Infiniforge.git"
  },
  "keywords": [
    "forge",
    "procedural",
    "generation",
    "sword",
    "threejs",
    "infiniforge"
  ],
  "author": "Shi Johnson-Bey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ShiJbey/Infiniforge/issues"
  },
  "homepage": "https://github.com/ShiJbey/Infiniforge#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "browserslist": "^4.6.6",
    "colors": "^1.3.3",
    "commander": "^6.1.0",
    "express": "^4.17.1",
    "lodash": "^4.17.20",
    "seedrandom": "^2.4.4",
    "serve-favicon": "^2.5.0",
    "serve-static": "^1.14.1",
    "three": "^0.120.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.8",
    "@types/lodash": "^4.14.161",
    "@types/node": "^10.14.17",
    "@types/seedrandom": "^2.4.28",
    "@types/serve-favicon": "^2.5.0",
    "@types/three": "^0.93.31",
    "tslint": "^5.19.0",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.2"
  }
}
